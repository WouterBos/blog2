<!-- Website mata -->
<%- partial ("./_head/meta.ejs") %>

<!-- Website rss -->
<% if (theme.rss === 'default' && config.feed && config.feed.path) { %>
<%- feed_tag(config.feed.path) %>
<% } else if (theme.rss) { %>
<%- feed_tag(theme.rss) %>
<% } else { %>
<%- feed_tag('atom.xml') %>
<% } %>

<!-- Theme styling -->
<link rel="stylesheet" type="text/css" href="<%- url_for('css/style.css') %>?v=<%- theme.version %>" />
